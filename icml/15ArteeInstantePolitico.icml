  <CharacterStyleRange AppliedCharacterStyle="$ID/NormalCharacterStyle">
    <Content>Figure 1: Ciro Cozzolino, </Content>
  </CharacterStyleRange>
  <CharacterStyleRange AppliedCharacterStyle="CharacterStyle/Italic">
    <Content>SP JUN 2013</Content>
  </CharacterStyleRange>
  <CharacterStyleRange AppliedCharacterStyle="$ID/NormalCharacterStyle">
    <Content> , 2013, acrílica sobre tela, 147,2 cm x 185,1 cm x 5 cm, Museu de Arte Contemporânea da Universidade de São Paulo -- MAC USP.</Content>
